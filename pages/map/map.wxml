<!--pages/map/map.wxml-->
<view class="page-body">
  <view class="page-secton page-section-gap">
    <map
      id="myMap"
      style="width: 100%; height: 350px;"
      latitude='{{latitude}}'
      longitude='{{longitude}}'
      markers='{{markers}}'
      covers='{{covers}}'
      show-location='ture'
    ></map>

  </view>
  <view class="info">
    <text>{{text}}</text>
  </view>
</view>
